  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0"
  },
  "author": "Krnos",
  "license": "EULA",
  "bugs": {
    "url": "https://github.com/Krnos/kronos/issues"
  },
  "homepage": "https://github.com/Krnos/kronos#readme",
  "dependencies": {
    "@vue/cli-shared-utils": "^5.0.1",
    "async": "^3.1.0",
    "change-case": "^4.1.1",
    "consolidate": "^0.16.0",
    "fs": "0.0.1-security",
    "fs-extra": "^11.1.0",
    "handlebars": "^4.5.1",
    "lodash": "^4.17.15",
    "metalsmith": "^2.3.0",
    "os": "^0.1.1",
    "path": "^0.12.7",
